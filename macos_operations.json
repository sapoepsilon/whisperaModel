{
  "_description": "Comprehensive macOS operations config for voice commands",
  "_version": "2.0",
  "_instructions": "Add operations here - they work immediately via pattern matching! Retraining optional for best accuracy.",

  "categories": {
    "apps": {
      "open": "open -a \"{app}\"",
      "quit": "osascript -e 'tell application \"{app}\" to quit'",
      "hide": "osascript -e 'tell application \"System Events\" to set visible of process \"{app}\" to false'",
      "show": "osascript -e 'tell application \"{app}\" to activate'"
    },

    "volume": {
      "set": "osascript -e 'set volume output volume {level}'",
      "up": "osascript -e 'set volume output volume ((output volume of (get volume settings)) + 10)'",
      "down": "osascript -e 'set volume output volume ((output volume of (get volume settings)) - 10)'",
      "mute": "osascript -e 'set volume output muted true'",
      "unmute": "osascript -e 'set volume output muted false'",
      "get": "osascript -e 'output volume of (get volume settings)'"
    },

    "audio": {
      "list_devices": "system_profiler SPAudioDataType",
      "list_output": "system_profiler SPAudioDataType | grep -A2 'Output'",
      "list_input": "system_profiler SPAudioDataType | grep -A2 'Input'",
      "switch_output": "SwitchAudioSource -s \"{device}\"",
      "switch_input": "SwitchAudioSource -t input -s \"{device}\"",
      "_comment": "switch commands require: brew install switchaudio-osx"
    },

    "brightness": {
      "set": "brightness {level}",
      "up": "osascript -e 'tell application \"System Events\" to key code 144'",
      "down": "osascript -e 'tell application \"System Events\" to key code 145'"
    },

    "display": {
      "sleep": "pmset displaysleepnow",
      "lock": "pmset displaysleepnow",
      "screenshot": "screencapture -i ~/Desktop/screenshot_$(date +%Y%m%d_%H%M%S).png",
      "screenshot_full": "screencapture ~/Desktop/screenshot_$(date +%Y%m%d_%H%M%S).png",
      "screenshot_window": "screencapture -w ~/Desktop/screenshot_$(date +%Y%m%d_%H%M%S).png",
      "screenshot_clipboard": "screencapture -c"
    },

    "system": {
      "sleep": "pmset sleepnow",
      "restart": "osascript -e 'tell app \"System Events\" to restart'",
      "shutdown": "osascript -e 'tell app \"System Events\" to shut down'",
      "logout": "osascript -e 'tell app \"System Events\" to log out'",
      "empty_trash": "osascript -e 'tell app \"Finder\" to empty trash'",
      "show_desktop": "osascript -e 'tell app \"System Events\" to key code 103 using {command down, fn down}'",
      "spotlight": "osascript -e 'tell app \"System Events\" to keystroke space using command down'",
      "notification_center": "osascript -e 'tell app \"System Events\" to keystroke \"n\" using {command down, option down}'",
      "force_quit_menu": "osascript -e 'tell app \"System Events\" to keystroke \"escape\" using {command down, option down}'"
    },

    "power": {
      "caffeinate": "caffeinate -d",
      "caffeinate_stop": "killall caffeinate",
      "battery_status": "pmset -g batt",
      "low_power_on": "sudo pmset -a lowpowermode 1",
      "low_power_off": "sudo pmset -a lowpowermode 0"
    },

    "network": {
      "wifi_off": "networksetup -setairportpower en0 off",
      "wifi_on": "networksetup -setairportpower en0 on",
      "wifi_status": "networksetup -getairportpower en0",
      "wifi_network": "networksetup -getairportnetwork en0",
      "ip_address": "ipconfig getifaddr en0",
      "public_ip": "curl -s ifconfig.me",
      "flush_dns": "sudo dscacheutil -flushcache"
    },

    "bluetooth": {
      "on": "blueutil -p 1",
      "off": "blueutil -p 0",
      "status": "blueutil -p"
    },

    "window": {
      "close": "osascript -e 'tell app \"System Events\" to keystroke \"w\" using command down'",
      "minimize": "osascript -e 'tell app \"System Events\" to keystroke \"m\" using command down'",
      "fullscreen": "osascript -e 'tell app \"System Events\" to keystroke \"f\" using {command down, control down}'",
      "new_tab": "osascript -e 'tell app \"System Events\" to keystroke \"t\" using command down'",
      "new_window": "osascript -e 'tell app \"System Events\" to keystroke \"n\" using command down'",
      "close_all": "osascript -e 'tell app \"System Events\" to keystroke \"w\" using {command down, option down}'",
      "cycle_windows": "osascript -e 'tell app \"System Events\" to keystroke \"`\" using command down'",
      "mission_control": "osascript -e 'tell app \"System Events\" to key code 126 using control down'"
    },

    "keyboard": {
      "copy": "osascript -e 'tell app \"System Events\" to keystroke \"c\" using command down'",
      "paste": "osascript -e 'tell app \"System Events\" to keystroke \"v\" using command down'",
      "cut": "osascript -e 'tell app \"System Events\" to keystroke \"x\" using command down'",
      "undo": "osascript -e 'tell app \"System Events\" to keystroke \"z\" using command down'",
      "redo": "osascript -e 'tell app \"System Events\" to keystroke \"z\" using {command down, shift down}'",
      "save": "osascript -e 'tell app \"System Events\" to keystroke \"s\" using command down'",
