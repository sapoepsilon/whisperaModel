    },

    "media": {
      "play_pause": "osascript -e 'tell app \"System Events\" to key code 49'",
      "next": "osascript -e 'tell app \"System Events\" to key code 124 using command down'",
      "previous": "osascript -e 'tell app \"System Events\" to key code 123 using command down'",
      "play": "osascript -e 'tell app \"Music\" to play'",
      "pause": "osascript -e 'tell app \"Music\" to pause'",
      "stop": "osascript -e 'tell app \"Music\" to stop'"
